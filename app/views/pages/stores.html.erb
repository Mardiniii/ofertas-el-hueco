<div class="container main-container headerOffset">

  <!-- Main component call to action -->

  <div class="row">
    <div class="breadcrumbDiv col-lg-12">
      <ul class="breadcrumb">
        <li> <a href="index.html">Home</a> </li>
        <li class="active">Tiendas </li>
      </ul>
    </div>
  </div>
  <!-- /.row  -->

  <div class="row">

    <!--left column-->

    <div class="col-lg-3 col-md-3 col-sm-12">
      <div class="panel-group" id="accordionNo">
        <!--Category-->
        <div class="panel panel-default">
          <div class="panel-heading">
            <h4 class="panel-title"> <a data-toggle="collapse"  href="#collapseCategory" class="collapseWill"> <span class="pull-left"> <i class="fa fa-caret-right"></i></span> Category </a> </h4>
          </div>
          <div id="collapseCategory" class="panel-collapse collapse in">
            <div class="panel-body">
              <ul class="nav nav-pills nav-stacked tree">
                <% @storehouses.each do |storehouse| %>
                  <% category_hash = storehouse_categories(storehouse) %>
                  <li class="active dropdown-tree" > <a  class="dropdown-tree-a" > <span class="badge pull-right"><%= category_hash.count %></span> <%= storehouse.name %> </a>
                    <ul class="category-level-2 dropdown-menu-tree">
                      <% category_hash.each do |category, subcategory| %>
                        <li class="dropdown-tree"> <a class="dropdown-tree-a"  href="#"> <%= category.name %></a>
                          <ul class="category-level-2 dropdown-menu-tree">
                            <% subcategory.each do |subcategory| %>
                              <li> <a href="#"><%= subcategory.name %></a> </li>
                            <% end %>
                          </ul>
                        </li>
                      <% end %>
                    </ul>
                  </li>
                <% end %>
              </ul>
            </div>
          </div>
        </div>
        <!--/Category menu end-->

